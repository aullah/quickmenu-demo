  "license": "MIT",
  "devDependencies": {
    "concurrently": "^5.3.0",
    "sass": "^1.32.7",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.22.0",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.2",
    "@popperjs/core": "^2.7.0",
    "bootstrap": "^5.0.0-beta2",
    "quickmenu": "^1.2.0"
  }
}
